@model List<Employee>
<section id="chefs" class="chefs">
    <div class="container" data-aos="fade-up">

        <div class="section-title">
            <h2>Personeller</h2>
            <p>Profesyonel Personellerimiz</p>
        </div>

        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-md-6">
                    <div class="member" data-aos="zoom-in" data-aos-delay="100">
                        <img src="@item.EmployeeImage" class="img-fluid" alt="">
                        <div class="member-info">
                            <div class="member-info-content">
                                <h4>@item.EmployeeName @item.EmployeeSurName</h4>
                                <span>Kurucu</span>
                            </div>
                            <div class="social">
                                <a href="@item.EmployeeTwitter"><i class="bi bi-twitter"></i></a>
                                <a href="@item.EmployeeFacebook"><i class="bi bi-facebook"></i></a>
                                <a href="@item.EmployeeInstagram"><i class="bi bi-instagram"></i></a>
                                <a href="@item.EmployeeLinkedin"><i class="bi bi-linkedin"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            }
            

        

        </div>

    </div>
</section>