    <header class="header-bar d-flex d-lg-block align-items-center" data-aos="fade-left">
        <div class="site-logo">
            <a href="/Image/AddImage/" class="active">Fotoğraf Yükle</a>
        </div>
        <div class="main-menu">
            <ul class="js-clone-nav">
                <li><a href="/Product/Index/">Ana Sayfaya Git</a></li>
                <li class="active"><a href="/Image/AddImage/">Fotoğraf Yükle</a></li>
                <li><a href="/Product/Index/#gallery">Fotoğraflara Git</a></li>
                <li><a href="/Product/Index/#book-a-table">Yorumlara Git</a></li>
                <li><a href="/Product/Index/#contact">İletişim Kısmına Git</a></li>
            </ul>

        </div>
    </header>
    <main class="main-content">
        <div class="container-fluid photos">
            <div class="row align-items-stretch">
                @foreach (var item in Model)
                {
                    <div class="col-6 col-md-6 col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="d-block photo-item">
                            <img src="@item.ImageUpload" alt="Image" class="img-fluid" data-pagespeed-url-hash="4024297802" onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
                            <div class="photo-text-more">
                                <div class="photo-text-more">
                                    <a class="heading btn btn-danger" href="/Image/DeleteImage/@item.ImageID">Fotoğrafı Sil</a>
                                    @*<a class="meta btn btn-success" style="margin-top:10px" href="/Image/UpdateImage/@item.ImageID">Fotoğrafı Güncelle</a>*@
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>

        </div>
    </main>

